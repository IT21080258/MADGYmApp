<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-1dp"
        android:background="@color/Gold"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/profPic"
            android:layout_width="121dp"
            android:layout_height="102dp"
            tools:ignore="ContentDescription"
            tools:layout_editor_absoluteX="273dp"
            tools:layout_editor_absoluteY="46dp"
            tools:srcCompat="@tools:sample/avatars" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_dashboard"
            android:textSize="28sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="45dp" />

        <TextView
            android:id="@+id/userName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="User Name"
            android:textSize="16sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="84dp" />

        <Button
            android:id="@+id/Logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Log Out"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="105dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/Membership"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="My Membership"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="200dp" />

    <Button
        android:id="@+id/Booking"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="My Bookings"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="300dp" />


    <Button
        android:id="@+id/Exercise"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="My exercises"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="400dp" />

    <Button
        android:id="@+id/Trainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Trainers"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="500dp" />



    <Button
        android:id="@+id/Other"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Other Activities"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="600dp" />



    <ImageView
        android:id="@+id/Dumbell"
        android:layout_width="100dp"
        android:layout_height="96dp"
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="696dp"
        tools:srcCompat="@drawable/Dumbell01"
        tools:ignore="ContentDescription,MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>