<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="418dp"
        android:layout_height="150dp"
        android:background="@color/Gold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-13dp"
        tools:layout_editor_absoluteY="0dp">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="158dp"
            android:layout_height="101dp"
            tools:ignore="ContentDescription"
            tools:layout_editor_absoluteX="130dp"
            tools:layout_editor_absoluteY="16dp"
            tools:srcCompat="@drawable/Logo" />

        <TextView
            android:id="@+id/Trainers01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/questiontitle"
            android:textColor="@color/Blue_001"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="105dp"
            tools:layout_editor_absoluteY="117dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="355dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/problem"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="190dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="247dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="130dp"
        android:layout_height="198dp"
        tools:layout_editor_absoluteX="232dp"
        tools:layout_editor_absoluteY="637dp"
        tools:srcCompat="@drawable/boxingbag"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/answers"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="369dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sampleanswer01"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="401dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sampleanswer02"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="434dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/question"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="341dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>